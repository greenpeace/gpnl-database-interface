{
	"name": "greenpeace/gpnl-database-interface",
	"description": "This repository contains the Interface for the GPNL Planet4 project to connect to our APIs",
	"type": "wordpress-plugin",
	"keywords": [
		"swagger",
		"php",
		"sdk",
		"api"
	],
	"license": "proprietary",
	"authors": [
		{
			"name": "Swagger and contributors",
			"homepage": "https://github.com/swagger-api/swagger-codegen"
		},
		{
			"name": "Oscar Keur",
			"email": "okeur@greenpeace.org"
		}
	],
	"require": {
		"php": ">=5.5",
		"ext-curl": "*",
		"ext-json": "*",
		"ext-mbstring": "*",
		"vlucas/phpdotenv": "^4.1",
		"guzzlehttp/guzzle": "^6.2"
	},
	"autoload": {
		"psr-4": {
			"P4NL_DATABASE_INTERFACE\\" : ["client/lib/",""]
		}
	},
	"autoload-dev": {
		"psr-4": { "P4NL_DATABASE_INTERFACE\\" : "client/test/" }
	}
}
