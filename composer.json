{
	"name": "greenpeace/gpnl-database-interface",
	"description": "",
	"keywords": [
		"swagger",
		"php",
		"sdk",
		"api"
	],
	"license": "proprietary",
	"authors": [
		{
			"name": "Swagger and contributors",
			"homepage": "https://github.com/swagger-api/swagger-codegen"
		},
		{
			"name": "Oscar Keur",
			"email": "okeur@greenpeace.org"
		}
	],
	"require": {
		"vlucas/phpdotenv": "^4.1",
		"wikimedia/composer-merge-plugin": "1.4.1"
	},
	"autoload": {
		"psr-4": {
			"P4NL_DATABASE_INTERFACE\\" : ["client/lib/",""]
		}
	},
	"autoload-dev": {
		"psr-4": { "P4NL_DATABASE_INTERFACE\\" : "client/test/" }
	},
	"extra": {
		"merge-plugin": {
			"require": "client/composer.json",
			"recurse": true,
			"replace": true,
			"ignore-duplicates": false,
			"merge-dev": true,
			"merge-extra": false,
			"merge-extra-deep": false,
			"merge-scripts": true
		}
	}
}
